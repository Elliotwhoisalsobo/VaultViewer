<Window x:Class="VaultViewer.UI.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VaultViewer.UI"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="CreateEmployeeBtn" Content="Add employee" HorizontalAlignment="Left" Margin="423,217,0,0" VerticalAlignment="Top"/>
        <Button x:Name="DeleteEmployeeBtn_Copy" Content="Delete employee" HorizontalAlignment="Left" Margin="423,264,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="EmployeeListBox"  d:ItemsSource="{d:SampleData ItemCount=5}"  Margin="290,200,400,122"/>  <!--SelectionChanged="" DisplayMemberPath="" IsSynchronizedWithCurrentItem="True"-->

    </Grid>
</Window>
